    "alerts": [
      {
        "type": "critical",
        "condition": "sms_delivery_rate < 90",
        "action": "immediate_notification",
        "recipients": ["admin@jkiaexpress.taxi", "+254731050573"]
      },
      {
        "type": "warning",
        "condition": "paypal_success_rate < 90",
        "action": "email_notification",
        "recipients": ["tech@jkiaexpress.taxi"]
      },
      {
        "type": "info",
        "condition": "monthly_target_variance > 10",
        "action": "weekly_report",
        "recipients": ["management@jkiaexpress.taxi"]
      }
    ],
    "reports": {
      "daily": {
        "time": "09:00",
        "recipients": ["operations@jkiaexpress.taxi"],
        "metrics": ["sms_delivery", "paypal_transactions", "bookings", "revenue"]
      },
      "weekly": {
        "day": "monday",
        "time": "08:00",
        "recipients": ["management@jkiaexpress.taxi"],
        "metrics": ["all_kpis", "budget_variance", "partnership_performance"]
      },
      "monthly": {
        "day": 1,
        "time": "09:00",
        "recipients": ["executives@jkiaexpress.taxi"],
        "metrics": ["comprehensive_analysis", "roi_assessment", "strategic_recommendations"]
      }
    }
  },
  "integration_endpoints": {
    "africastalking": {
      "base_url": "https://api.africastalking.com",
      "metrics_endpoint": "/version1/user",
      "auth_method": "api_key"
    },
    "paypal": {
      "base_url": "https://api-m.paypal.com",
      "metrics_endpoint": "/v1/reporting/transactions",
      "auth_method": "oauth2"
    },
    "google_analytics": {
      "property_id": "GA_PROPERTY_ID",
      "metrics_endpoint": "/v4/reports:batchGet",
      "auth_method": "service_account"
    },
    "google_my_business": {
      "account_id": "GMB_ACCOUNT_ID",
      "metrics_endpoint": "/v4/accounts/{accountId}/locations",
      "auth_method": "oauth2"
    }
  },
  "data_retention": {
    "raw_data": "90_days",
    "aggregated_data": "2_years",
    "reports": "5_years"
  }
}